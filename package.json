    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "lucide-react": "^0.545.0",
    "morgan": "^1.10.1",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^2.15.4",
    "sequelize": "^6.37.7",
