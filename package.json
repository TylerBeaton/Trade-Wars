{
  "name": "tradewars",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsx watch server.ts",
    "dev": "nodemon --watch ./ --ext ts --exec tsx server.ts",
    "build": "next build",
    "test": "cross-env NODE_ENV=test mocha --require ts-node/register tests/first.test.ts tests/userModel.test.ts tests/userRoutes.test.ts tests/tradeRoutes.test.ts --timeout 10000 --reporter spec --bail",
    "test:routes": "cross-env NODE_ENV=test mocha --require ts-node/register tests/userRoutes.test.ts tests/gameRoutes.test.ts",
    "test:models": "cross-env NODE_ENV=test mocha --require ts-node/register tests/userModel.test.ts tests/tradeModel.test.ts",
    "lint": "next lint",
    "db:seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
    "db:reset": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts --reset"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
