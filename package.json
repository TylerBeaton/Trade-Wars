    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.15",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "chai": "^5.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.31.0",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
